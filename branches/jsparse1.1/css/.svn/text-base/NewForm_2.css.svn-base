SELECT.inputSelect {
	BORDER-RIGHT: #333333 1px solid; PADDING-RIGHT: 1px; BORDER-TOP: #333333 1px solid; PADDING-LEFT: 1px; FONT-SIZE: 0.7em; PADDING-BOTTOM: 1px; BORDER-LEFT: #333333 1px solid; COLOR: #000000; PADDING-TOP: 1px; BORDER-BOTTOM: #333333 1px solid; BACKGROUND-COLOR: #ffffe6
}
SELECT.inputSelect-Disabled {
	BORDER-RIGHT: #999999 1px solid; PADDING-RIGHT: 1px; BORDER-TOP: #999999 1px solid; PADDING-LEFT: 1px; FONT-SIZE: 0.7em; PADDING-BOTTOM: 1px; BORDER-LEFT: #999999 1px solid; COLOR: #999999; PADDING-TOP: 1px; BORDER-BOTTOM: #999999 1px solid; BACKGROUND-COLOR: #fefefe
}
SELECT.inputSelect-ReadOnly {
	BORDER-RIGHT: #666666 1px solid; PADDING-RIGHT: 1px; BORDER-TOP: #666666 1px solid; PADDING-LEFT: 1px; FONT-SIZE: 0.7em; PADDING-BOTTOM: 1px; BORDER-LEFT: #666666 1px solid; COLOR: #666666; PADDING-TOP: 1px; BORDER-BOTTOM: #666666 1px solid; BACKGROUND-COLOR: #ffffe6
}
SELECT.inputSelect-Required {
	BORDER-RIGHT: #ff0000 1px solid; PADDING-RIGHT: 1px; BORDER-TOP: #ff0000 1px solid; PADDING-LEFT: 1px; FONT-SIZE: 0.7em; PADDING-BOTTOM: 1px; BORDER-LEFT: #ff0000 1px solid; COLOR: #000000; PADDING-TOP: 1px; BORDER-BOTTOM: #ff0000 1px solid; BACKGROUND-COLOR: #ffffe6
}
LABEL.labelSelect {
	PADDING-RIGHT: 1px; DISPLAY: block; PADDING-LEFT: 1px; FONT-SIZE: 0.7em; PADDING-BOTTOM: 1px; MARGIN: 0px; COLOR: #666666; PADDING-TOP: 6px
}
LABEL.labelSelect-Disabled {
	PADDING-RIGHT: 1px; DISPLAY: block; PADDING-LEFT: 1px; FONT-SIZE: 0.7em; PADDING-BOTTOM: 1px; MARGIN: 0px; COLOR: #666666; PADDING-TOP: 6px
}
LABEL.labelSelect-ReadOnly {
	PADDING-RIGHT: 1px; DISPLAY: block; PADDING-LEFT: 1px; FONT-SIZE: 0.7em; PADDING-BOTTOM: 1px; MARGIN: 0px; COLOR: #666666; PADDING-TOP: 6px
}
LABEL.labelSelect-Required {
	PADDING-RIGHT: 1px; DISPLAY: block; PADDING-LEFT: 1px; FONT-SIZE: 0.7em; PADDING-BOTTOM: 1px; MARGIN: 0px; COLOR: #666666; PADDING-TOP: 6px
}
LABEL.labelSelect-Disabled {
	COLOR: #999999
}
LABEL.labelSelect-ReadOnly {
	COLOR: #666666
}
LABEL.labelSelect-Required {
	COLOR: #ff0000
}
