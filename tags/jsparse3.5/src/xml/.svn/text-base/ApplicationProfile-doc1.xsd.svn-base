												<xs:attribute name="enabled" type="xs:boolean"/>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
									<xs:attribute name="startUpTimeout" type="xs:positiveInteger" default="60">
										<xs:annotation>
            								<xs:documentation>
												Specifies the number of seconds to wait for the session manager to start up before the 
												process times out and the session director tries again to start a new session manager.
												Range: 1 or more
											</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="shutDownTimeout" type="xs:positiveInteger" default="300">
										<xs:annotation>
            								<xs:documentation>
												Specifies the number of seconds to wait for the session manager to shut down before the 
												session director considers it as timed out and restarts the session manager.
												Range: 1 or more
											</xs:documentation>
										</xs:annotation>
