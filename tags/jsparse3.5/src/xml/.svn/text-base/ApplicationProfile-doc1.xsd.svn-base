												<xs:maxInclusive value="2147483647"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
									<xs:attribute name="taskRetryLimit" use="optional" default="3">
										<xs:annotation>
            								<xs:documentation>
