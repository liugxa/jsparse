/* TR.controlRow */
TABLE TD.controlRow {
	BACKGROUND-COLOR: #ffffff! important
}

/* TR.paramRowXmlL */
TABLE TR.paramRowXmlL TD {
	BORDER-TOP: #ffffff 2px solid; 
	
	BACKGROUND-COLOR: #ffff44
}
TABLE TR.paramRowXmlL TD.paramValueRequired {
	BORDER-RIGHT: #ff0000 1px solid; BORDER-LEFT: #ff0000 1px solid; 
	
	BACKGROUND-COLOR: #ffcc00
}
TABLE TR.paramRowXmlL TD.paramName {
	PADDING-RIGHT: 12px; FONT-WEIGHT: normal; COLOR: #000000; PADDING-LEFT: 16px; 

	BACKGROUND-REPEAT: no-repeat;
	BACKGROUND-POSITION: left top; 
	BACKGROUND-COLOR: #efefef;

	/* BACKGROUND-IMAGE: url(../images/icon-L1.gif) */
}

/* TR.paramRowDefaultL */
TABLE TR.paramRowDefaultL TD {
	BORDER-TOP: #ffffff 2px solid; BACKGROUND-COLOR: #dddddd
}
TABLE TR.paramRowDefaultL TD.paramName {
	PADDING-RIGHT: 12px; FONT-WEIGHT: normal; COLOR: #000000;PADDING-LEFT: 16px;
	
	BACKGROUND-REPEAT: no-repeat;
	BACKGROUND-POSITION: left top; 
	BACKGROUND-COLOR: #efefef;

	/*BACKGROUND-IMAGE: url(../images/icon-L2.gif)*/
}


/* TR.containerRowXmlL */
TABLE TR.containerRowXmlL TD {
	BORDER-TOP: #ffffff 2px solid; COLOR: #ffffff;
	BACKGROUND-COLOR: #0000aa
}
TABLE TR.containerRowXmlL TD.paramName {
	PADDING-RIGHT: 12px; FONT-WEIGHT: bold; COLOR: #ffffff! important;PADDING-LEFT: 16px;  

	BACKGROUND-POSITION: left top; 
	BACKGROUND-REPEAT: no-repeat;
	BACKGROUND-IMAGE: url(../images/icon-L1.gif)
}

