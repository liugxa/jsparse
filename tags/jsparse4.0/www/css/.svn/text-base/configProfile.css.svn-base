/* ---------------------------------------------------------------------- *//* ---------------------------------------------------------------------- *//* ---------------------------------------------------------------------- *//* Format pages which present forms for editing a profile *//* ------------------------------------------------------------------ *//* Basic styles */div.formConfigProfile {	padding: 0px;	margin: 0px;	padding-left: 0em;	padding-top: 6px;}div.formConfigProfile div {	padding-left: 1em;}div.formConfigProfile h2 {	font-size: .9em;	padding: 0px;	padding-top: 2px;	padding-bottom: 4px;	margin: 0px;}div.formConfigProfile p {	margin: 0px;	padding-top: 0px;}div.formConfigProfile div.charts {    font-size: 1px;	padding-left: 0em;}div.formConfigProfile div.charts img {	margin: 0px;	margin-right: 6px;	margin-bottom: 6px;	padding: 0px;	border-style: solid;	border-width: 1px;	border-color: #999999;}/* ------------------------------------------------------------------------ *//* Format a container fieldset and div */div.formConfigProfile fieldset {	padding: 3px;	margin: 0px;	background-color: #FDFDFD;	border-color: #999999;	border-style: solid;	border-width: 1px;	margin-bottom: 12px;}div.formConfigProfile fieldset legend {	padding: 0px;	padding-right: 3px;	padding-left: 3px;	margin: 0px;	font-size: 0.7em;	font-weight: bold;}/* ------------------------------------------------------------------------ *//* Present repeating parameters (e.g., environment variables) in a table */fieldset.propertiesAppProfile div.scrollableList {}table.tableVariables {	font-size: .7em;	text-align: left;	padding: 0px;	margin: 0px;	margin-top: 6px;}table.tableVariables th {	font-weight: normal;	padding: 0px;	padding-right: 2px;	padding-left: 2px;	border-top-style: solid;	border-top-width: 1px;	border-top-color: #999999;	border-bottom-style: solid;	border-bottom-width: 1px;	border-bottom-color: #999999;}table.tableVariables tbody {	font-size: 100%;}table.tableVariables tbody td {	padding-top: 2px;	padding-right: 2px;	padding-bottom: 0px;	padding-left: 2px;}table.tableVariables input.inputTextField {	font-size: 100% !important;}table.tableVariables select {	font-size: 100% !important;}table.tableVariables label.labelTextField,table.tableVariables label.labelSelect {	display: none !important;}/* ------------------------------------------------------------------------ *//* Main containers (fieldsets and divs) *//* Format nested fieldsets and divs with alternating even/odd boxes that change color */div.formConfigProfile fieldset.formSection-Neutral,div.formConfigProfile fieldset.formSection-Even,div.formConfigProfile fieldset.formSection-Odd {	display: block; 	background-color: transparent;		padding: 0px !important;	padding-bottom: 5px !important;	padding-left: 0px !important;		margin: 0px;	margin-top: 10px;		border-style: solid;	border-width: 1px;	border-color: #dddddd;	}/* Eliminate extra padding and margings on "neutral" fieldsets */div.formConfigProfile fieldset.formSection-Neutral {	padding: 0px;		margin: 0px;	margin-top: 0px;		border: none;}/* Specify alternating background colors */div.formConfigProfile fieldset.formSection-Even {	background-color: #ffffff;}div.formConfigProfile fieldset.formSection-Odd {	background-color: #efefef;}/* Format spacing on first nested div within fieldsets */div.formConfigProfile fieldset.formSection-Neutral div,div.formConfigProfile fieldset.formSection-Even div,div.formConfigProfile fieldset.formSection-Odd div {	display: block;		padding-top: 0px;	padding-right: 10px;	padding-bottom: 5px;	padding-left: 10px;		margin: 0px;}/* Format spacing on first nested div within fieldsets */div.formConfigProfile fieldset.formSection-Neutral div div,div.formConfigProfile fieldset.formSection-Even div div,div.formConfigProfile fieldset.formSection-Odd div div {	display: block;		padding-top: 0px;	padding-right: 0px;	padding-bottom: 5px;	padding-left: 0px;		margin: 0px;}/* Manage extra padding and margings on nested "neutral" fieldset divs */div.formConfigProfile fieldset.formSection-Neutral div {	padding: 0px;	margin: 0px;}div.formConfigProfile fieldset.formSection-Neutral div div {	padding: 0px;	margin: 0px;	padding-right: 10px;	padding-left: 10px;}div.formConfigProfile fieldset.formSection-Neutral div div div {	padding: 0px;	margin: 0px;}div.formConfigProfile fieldset.formSection-Neutral  div div div div {	padding: 0px;	margin: 0px;	padding-right: 10px;	padding-left: 10px;}/* ------------------------------------------------------------------------ *//* Format "Specify View" fieldset, which contains controls for    specifying visibility of instances of multiple objects (e.g., service types) */div.formConfigProfile fieldset.formSection-Neutral fieldset.formSpecifyView,div.formConfigProfile fieldset.formSection-Even fieldset.formSpecifyView,div.formConfigProfile fieldset.formSection-Odd fieldset.formSpecifyView {	display: block; 	background-color: transparent;		padding: 5px;	padding-left: 10px;	margin: 0px;	margin-top: 0px;		border: none !important;		border-bottom-style: solid !important;	border-bottom-width: 4px !important;	border-bottom-color: #cccccc;}/* Handle annoying rendering bug in IE */div.formConfigProfile fieldset.formSection-Even fieldset.formSpecifyView {	border-top-color: #ffffff !important;	border-bottom-color: #efefef !important;}div.formConfigProfile fieldset.formSection-Odd fieldset.formSpecifyView {	border-top-color: #efefef !important;	border-bottom-color: #ffffff !important;}/* Handle buttons container */div.formConfigProfile fieldset.formSpecifyView div.formSpecifyView {	float: right;	padding: 0px;	margin: 0px;	white-space: nowrap;}div.formConfigProfile fieldset.formSpecifyView div.formSpecifyView button {	display: inline;}/* Specify visibility of legend for "Specify View" fieldset */div.formConfigProfile fieldset.formSection-Even fieldset.formSpecifyView legend,div.formConfigProfile fieldset.formSection-Odd fieldset.formSpecifyView legend {	display: none; }/* Specify visibility of label in "Specify View" fieldset */div.formConfigProfile fieldset.formSection-Even fieldset.formSpecifyView label,div.formConfigProfile fieldset.formSection-Odd fieldset.formSpecifyView label {	display: none; }/* Format buttons that control which object instance is visible */fieldset.formSpecifyView button {	display: inline; }/* Format menu (select) */fieldset.formSpecifyView select {	float: left; }/* Specify visibility of legend for an instance (e.g., of a service or OS type) */legend.formObjectInstance {	display: none;}/* ------------------------------------------------------------------------ *//* Format a legend to indicate that fieldset can expand and contract */legend a.formLinkExpandThis {	background-image:      url(../images/icon-link-ec-expand.gif);	background-repeat: no-repeat;	background-position: left center;	padding-left: 13px;	padding-right: 6px;}legend a.formLinkContractThis {	background-image:      url(../images/icon-link-ec-contract.gif);	background-repeat: no-repeat;	background-position: left center;	padding-left: 13px;	padding-right: 6px;}/* ---------------------------------------------------------------------- *//* Format presentation of multiple elements */div.multipleElements {	position: relative;}div.addAnElement {	background-color: #eeeeee;	border-width: 1px;	border-style: solid;	border-color: #999999;		position: absolute;	top: 0px;	right: 0px;		float: right;		padding: 6px !important;		z-index: 400;}div.addAnElement p {	width: 15em;}div.addAnElement buttom {	width: 15em;	font-size: .6em;}/* ---------------------------------------------------------------------- *//* Format "Admin" content */div.pageAdmin {	text-align: right;	margin: 0px;	padding: 0px;	padding-top: 0em;	padding-bottom: .25em;	float: right;}div.pageAdmin p {	display: inline;	margin: 0px;	padding: 0px;	padding-left: 1em;}div.pageAdmin p a.embeddedHelpEnabled {	background-image: url(../images/icon-help-embedded.gif);	background-position: center left;	background-repeat: no-repeat;		padding-left: 14px;	font-weight: normal;}div.pageAdmin p a.embeddedHelpDisabled {	background-image: url(../images/icon-help-embedded.gif);	background-position: center left;	background-repeat: no-repeat;		padding-left: 14px;	font-weight: normal;}div.pageAdmin p a.showTablePrefs {	background-image: url(../images/icon-link-panel.gif);	background-position: center left;	background-repeat: no-repeat;		padding-left: 14px;	font-weight: normal;}/* ------------------------------------------------------------------------ *//* Format "help" icon image (visible/enabled)  */img.embeddedHelpIcon {	margin: 0px;	margin-top: 1px;	padding: 0px;	/* padding-top: 1px; */	/* display: none; */	vertical-align: top;	border: none;	/*display: none;*/}/* ------------------------------------------------------------------------ *//* Format error messages for invalid input. */label span.formError,label span.formErrorExtra {	display: block; 	color: red; 	font-weight: bold;}label span.formError span.formErrorToggle {	display: inline; 	color: black; 	font-weight: normal;}